graph indiscernibility_example {
  node [color="black", fillcolor="/pubu9/1", style="filled"];
  //edge [style="invis"];
  
  subgraph cluster_abcd {
    color="black";
    fillcolor="/pubu9/2";
    label = "{}"; //{{}}
    style="filled";
    
    subgraph cluster_abc {
      color="black";
      fillcolor="/pubu9/3";
      label = "{P}"; //{{},{P}}
      style="filled";
      
      subgraph cluster_ab {
        color="black";
        fillcolor="/pubu9/4";
        label = "{P,Q}"; //{{},{P},{Q},{P,Q}}
        style="filled";
        node_a [label="a: {P,Q}"];
        node_b [label="b: {P,Q}"];
        node_a -- node_b;
      }
      
      node_c [label="c: {P}"];
      node_a -- node_c;
      node_b -- node_c;
    }
    
    node_d [label="d: {R}"];
    
    node_a -- node_d;
    node_b -- node_d;
    node_c -- node_d;
  }
  
  charset="UTF-8"
  fontsize="11.0"
  //label="Example showing an indiscernability partition for pairs of constances."
  overlap="false"
}

